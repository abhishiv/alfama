{
  "name": "dylanjs",
  "version": "0.0.8",
  "description": "Fine-grained reactive library in 5kb with no compiler, no magic, and no virtual DOM",
  "keywords": [
    "reactive",
    "dom",
    "tsx",
    "frontend",
    "framework"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/abhishiv/dylanjs"
  },
  "main": "./src/index.ts",
  "files": [
    "/dist"
  ],
  "scripts": {
    "test": "vitest",
    "coverage": "vitest run --coverage"
  },
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "require": "./src/index.ts"
    },
    "./dom": {
      "import": "./src/dom.ts",
      "require": "./src/dom.ts"
    },
    "./state": {
      "import": "./src/state.ts",
      "require": "./src/state.ts"
    }
  },
  "author": "Abhishiv Saxena<abhishiv@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "typescript": "^4.9.5",
    "vite": "^4.1.4",
    "vitest": "^0.28.5"
  },
  "dependencies": {}
}
