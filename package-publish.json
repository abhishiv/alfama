{
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/esm/index.d.ts"
    },
    "./dom": {
      "import": "./dist/esm/dom.js",
      "require": "./dist/cjs/dom.js",
      "types": "./dist/esm/dom.d.ts"
    },
    "./state": {
      "import": "./dist/esm/experimental/state.js",
      "require": "./dist/cjs/experimental/state.js",
      "types": "./dist/esm/experimental/state.d.ts"
    },
    "./stdlib": {
      "import": "./dist/esm/stdlib/index.js",
      "require": "./dist/cjs/stdlib/index.js",
      "types": "./dist/esm/stdlib/index.d.ts"
    }
  }
}
